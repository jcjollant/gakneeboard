{ 
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "headers": [       
        {
            "source": "/(.*)",
            "methods":["POST","OPTIONS"],
            "headers": [
                { "key": "Access-Control-Allow-Origin", "value": "*" }
            ]
        }
    ]
}
